	this.onChange = function(resolve) {
		changeFuncs.push(resolve);
	}

    this.complete = function(resolve) {
        if(completed){
            resolve(json);
        }else{
            this.onChange(function(data) {
                if(completed){
                    resolve(data);
                }
            })
        }
    };

    // Update this instance with the initial JSON payload.
    update.call(this, json);
